{
  "manifest_version": 3,
  "name": "Fear & Greed Index Tracker & VIX",
  "version": "1.2.1",
  "description": "Track CNN's Fear & Greed Index daily",
  "permissions": [
    "storage",
    "alarms"
  ],
  "host_permissions": [
    "https://production.dataviz.cnn.io/*",
    "*://*.investing.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}